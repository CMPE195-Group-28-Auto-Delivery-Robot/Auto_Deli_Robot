<launch>
    <group ns="gps_example">
        <node pkg="nmea_navsat_driver" name="NMEA_GPS" type="nmea_serial_driver">
            <param name="port" value="/dev/ttyACM0"/>
            <param name="baud" value="9600"/>
            <param name="frame_id" value="gps" />
            <param name="use_GNSS_time" value="False" />
            <param name="time_ref_source" value="gps" />
            <param name="useRMC" value="False" />
        </node>
        <node pkg="gps_module" name="Reporter" type="gps_talker_node" output="screen"/>
    </group>
</launch>